{
  "name": "material-anti-mage-vue",
  "author": "bre97-web",
  "private": false,
  "version": "0.0.2",
  "displayName": "Material Anti-Mage Vue",
  "keywords": [
    "vue",
    "components",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bre97-web/material-anti-mage-vue.git"
  },
  "bugs": {
    "url": "https://github.com/bre97-web/material-anti-mage-vue/issues"
  },
  "homepage": "https://github.com/bre97-web/material-anti-mage-vue",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "types": "/dist/index.d.ts",
  "files": [
    "dist/**/*",
    "!src/**/*",
    "!scripts/**/*",
    "!test-reporter/**/*",
    "!.vscode/**/*",
    "!catalog/**/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && eslint ./dist/**/*",
    "preview": "vite preview",
    "eslint": "npx eslint ./src/**",
    "eslint:fix-src": "npx eslint ./src/** --fix",
    "eslint:fix-dist": "npx eslint ./dist/** --fix",
    "template": "node ./scripts/generate.js",
    "test": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "aphrodite": "^2.4.0",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@material/material-color-utilities": "^0.2.7",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.2",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.55.0",
    "generate-template-files": "^3.2.1",
    "jsdom": "^23.0.1",
    "material-symbols": "^0.14.2",
    "readline": "^1.3.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.0.2",
    "vue-tsc": "^1.8.25"
  }
}